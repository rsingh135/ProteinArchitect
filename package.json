{
  "name": "genlab",
  "version": "1.0.0",
  "type": "module",
  "description": "AI-Powered Synthetic Organism Design Platform",
  "private": true,
  "scripts": {
    "dev": "npm --prefix frontend run dev",
    "build": "npm --prefix frontend run build",
